{
  "compilerOptions": {
    "target": "esnext",
    "allowSyntheticDefaultImports": false,
    "baseUrl": "./",
    "paths": {
      "src/*": ["src/*"],
      "pages/*": ["src/pages/*"],
      "components/*": ["src/components/*"],
      "layouts/*": ["src/layouts/*"],
      "images/*": ["src/images/*"],
      "ui/*": ["src/components/ui/*"]
    }
  },
  "exclude": ["node_modules", "public", ".cache"]
}
